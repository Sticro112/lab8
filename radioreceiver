#include <iostream>
#include <string>

class Radioreceiver {
private:
    double working_frequency;
    double sensitivity;
    std::string modulation_type;

public:
    Radioreceiver() {
        working_frequency = 0;
        sensitivity = 0;
        modulation_type = "undefined";
    }

    Radioreceiver(double wf, double st, const std::string& mt) {
        set_wf(wf);
        set_st(st);
        set_mt(mt);
    }

    ~Radioreceiver() {
    }

    void set_wf(double wf) {
        if (wf > 0)
            working_frequency = wf;
        else
            working_frequency = 404;
    }

    void set_st(double st) {
        if (st > 0)
            sensitivity = st;
        else
            sensitivity = 404;
    }

    void set_mt(const std::string& mt) {
        if (!mt.empty())
            modulation_type = mt;
        else
            modulation_type = "ERROR";
    }

    void print() const {
        std::cout << "  WF: " << working_frequency
                  << "  ST: " << sensitivity
                  << "  MT: " << modulation_type << std::endl;
    }

     void print_range(){
        int number;
        std::cin >> number;
        if (number <= working_frequency)
        std::cout << "wf out of the border" << std::endl;
        else
        std::cout << "correct" << std::endl;
    }
};

void print_comment(){
        std::string comment;
        std::cin >> comment;
    }

void changeRadioreceiver(Radioreceiver& r, double wf, double st, const std::string& mt) {
    r.set_wf(wf);
    r.set_st(st);
    r.set_mt(mt);
}

int main() {
    Radioreceiver r1(1000, 10, "FM");

    r1.print();

    Radioreceiver r2(600, 0, "WI");

    r2.print();

    changeRadioreceiver(r1, 0, 200, "");

    r1.print();

    r2.print_range();

    print_comment();


    return 0;
}
